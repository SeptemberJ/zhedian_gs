<view class="Insert"><view class="TopInfo"><view class="item">日期：<picker style="display:inline-block;-align:center;" mode="date" value="{{FDate}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{FDate}}</view></picker></view><view class="item">项目：<input-autocomplete class="unit-item__input" style="width:70%;display:inline-block;text-align:left;border-bottom:1px solid #000000;" vue-id="1" value="{{FBase1Txt}}" placeholder="请输入" highlightColor="#FF0000" loadData="{{loadAutocompleteData1}}" data-event-opts="{{[['^updateSelect1',[['updateSelect1']]],['^selectItem',[['selectItemD1']]],['^input',[['__set_model',['','FBase1Txt','$event',[]]]]]]}}" bind:updateSelect1="__e" bind:selectItem="__e" bind:input="__e" bind:__l="__l"></input-autocomplete></view><view class="item">新项目编号：<input-autocomplete class="unit-item__input" style="width:70%;display:inline-block;text-align:left;border-bottom:1px solid #000000;" vue-id="2" value="{{FBase2Txt}}" placeholder="请输入" highlightColor="#FF0000" loadData="{{loadAutocompleteData2}}" data-event-opts="{{[['^updateSelect2',[['updateSelect2']]],['^selectItem',[['selectItemD2']]],['^input',[['__set_model',['','FBase2Txt','$event',[]]]]]]}}" bind:updateSelect2="__e" bind:selectItem="__e" bind:input="__e" bind:__l="__l"></input-autocomplete></view></view><view class="ListColumn"><text style="width:10%;">序号</text><text style="width:40%;">员工</text><text style="width:40%;">工时</text><text class="ColorWhite" style="width:10%;">删除</text></view><view class="ListBlock"><view class="ListMain"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="idx" wx:key="idx"><view style="margin-bottom:0rpx;"><view class="ListItem"><text style="width:10%;">{{idx+1}}</text><input-autocomplete class="unit-item__input" style="width:40%;text-align:center;border-bottom:0px solid #CCCCCC;" vue-id="{{'3-'+idx}}" value="{{item.fname}}" placeholder="请输入" highlightColor="#FF0000" loadData="{{loadAutocompleteData3}}" data-event-opts="{{[['^updateSelect3',[['updateSelect3']]],['^selectItem',[['selectItemEmp',[idx]]]],['^input',[['__set_model',['$0','fname','$event',[]],[[['listData','',idx]]]]]]]}}" bind:updateSelect3="__e" bind:selectItem="__e" bind:input="__e" bind:__l="__l"></input-autocomplete><input style="width:40%;text-align:center;border-bottom:0px solid #CCCCCC;" placeholder="请输入" data-event-opts="{{[['input',[['__set_model',['$0','FDecimal','$event',[]],[[['listData','',idx]]]]]]]}}" value="{{item.FDecimal}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['deleteLine',[idx]]]]]}}" style="width:10%;" bindtap="__e"><image style="width:40rpx;height:40rpx;display:block;margin:5rpx auto;" src="../../static/images/delete.png"></image></view></view></view></block><block wx:if="{{listData.length==0}}"><text class="EmptyData">暂无数据</text></block></view></view><view class="AddLine"><image style="width:40rpx;height:40rpx;display:block;margin:10rpx auto;" src="../../static/images/add.png" data-event-opts="{{[['tap',[['AddLine',['$event']]]]]}}" bindtap="__e"></image></view><button class="SubmitBt" disabled="{{ifNoWork}}" data-event-opts="{{[['tap',[['save',['$event']]]]]}}" bindtap="__e">保 存</button></view>